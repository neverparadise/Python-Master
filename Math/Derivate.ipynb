   "source": [
    "def f_x(x):\n",
    "    return x ** 2\n",
    "def derivate(f_x, x):\n",
    "    dx = 0.000001\n",
    "    f_prime = (f_x(x + dx) - f_x(x)) / dx\n",
    "    return f_prime"
   ]
